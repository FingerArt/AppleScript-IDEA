targetVariable ::= identifier
{mixin="com.idea.plugin.applescript.psi.impl.BaseAppleScriptComponent"
implements="com.idea.plugin.applescript.psi.AppleScriptComponent"}

private ifStatement ::= ifCompoundStatement|ifSimpleStatement

ifSimpleStatement ::= if expression then (/*!(ifCompoundStatement)*/ (statement|expression) )
//                                    {pin=4}
//todo: need to fix issue with detection simple vs compound statement
ifCompoundStatement ::= if expression [then] /*(then (statement|expression) | [then])*/ sep
                                       blockBody? nls
                                   (else if expression [then] nls
                                       blockBody? nls )*
                                   (else nls
                                        blockBody? nls)?
                                   end [if] {pin=9}

